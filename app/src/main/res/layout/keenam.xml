<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/android_6"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_centerHorizontal="false"
    android:background="@color/colorPrimary"
    >

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@color/colorAccent" />

    <Button
        android:id="@+id/back_3"
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:layout_alignStart="@+id/rectangle_4"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/ic_arrow_back_black_24dp" />

    <TextView
        android:id="@+id/tambah"
        android:layout_width="65dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="14dp"
        android:gravity="top"
        android:text="@string/tambah"
        android:textAppearance="@style/tambah" />

    <View
        android:id="@+id/rectangle_8"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="162dp"
        android:layout_marginTop="66dp"
        android:background="@color/colorAccent" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/rectangle_8"
        android:layout_marginTop="147dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nama Jajanan Pasar" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/rectangle_8"
        android:layout_marginTop="229dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Harga Jual" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/rectangle_8"
        android:layout_marginTop="317dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Deskripsi" />

    <Button
        android:id="@+id/button3"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/tambah"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="535dp"
        android:background="#C5F3C7"
        android:text="SIMPAN" />

    <Button
        android:id="@+id/jajan"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_below="@+id/rectangle_5"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="238dp"
        android:layout_marginLeft="238dp"
        android:layout_marginTop="137dp"
        android:layout_marginEnd="138dp"
        android:background="@drawable/image8tambahproduk" />



</RelativeLayout>